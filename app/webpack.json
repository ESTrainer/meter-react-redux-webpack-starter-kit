{
  "root": "src",
  "devServer": {
    "host": "localhost"
  },
  "devtool": "eval",
  "resolve": {
    "alias": {
      "App": "src/IDEApp/client/app",
      "Main": "src/MainApp/client/app",
      "react$": "./node_modules/react"
    },
    "modules": [
      "app",
      "components",
      "screens",
      "shared",
      "node_modules"
    ]
  },
  "externals": {
    "react": true,
    "react-dom": "ReactDOM",
    "react-router": "ReactRouter"
  }
}